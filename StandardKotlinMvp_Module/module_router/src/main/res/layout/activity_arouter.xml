<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView android:id="@+id/arouter_tv_main"
            android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="跳转主项目activity"
              android:textSize="16sp"
              android:textColor="@color/COLOR_333333"
              android:layout_marginTop="30dp"
              android:padding="5dp"
              android:background="@color/COLOR_41D282"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintTop_toTopOf="parent"/>

    <TextView android:id="@+id/arouter_tv_more"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="跳转主项目营养元素activity"
              android:textSize="16sp"
              android:textColor="@color/COLOR_333333"
              android:layout_marginTop="30dp"
              android:padding="5dp"
              android:background="@color/COLOR_41D282"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@id/arouter_tv_main" />

</androidx.constraintlayout.widget.ConstraintLayout>